/dts-v1/;

/ {
	interrupt-parent = <0x1>;
	#address-cells = <0x1>;
	#size-cells = <0x1>;
	compatible = "hardkernel,odroid-x", "samsung,exynos4412", "samsung,exynos4";
	model = "Hardkernel ODROID-X board based on Exynos4412";

	aliases {
		spi0 = "/soc/spi@13920000";
		spi1 = "/soc/spi@13930000";
		spi2 = "/soc/spi@13940000";
		i2c0 = "/soc/i2c@13860000";
		i2c1 = "/soc/i2c@13870000";
		i2c2 = "/soc/i2c@13880000";
		i2c3 = "/soc/i2c@13890000";
		i2c4 = "/soc/i2c@138a0000";
		i2c5 = "/soc/i2c@138b0000";
		i2c6 = "/soc/i2c@138c0000";
		i2c7 = "/soc/i2c@138d0000";
		i2c8 = "/soc/i2c@138e0000";
		csis0 = "/soc/camera/csis@11880000";
		csis1 = "/soc/camera/csis@11890000";
		fimc0 = "/soc/camera/fimc@11800000";
		fimc1 = "/soc/camera/fimc@11810000";
		fimc2 = "/soc/camera/fimc@11820000";
		fimc3 = "/soc/camera/fimc@11830000";
		serial0 = "/soc/serial@13800000";
		serial1 = "/soc/serial@13810000";
		serial2 = "/soc/serial@13820000";
		serial3 = "/soc/serial@13830000";
		pinctrl0 = "/soc/pinctrl@11400000";
		pinctrl1 = "/soc/pinctrl@11000000";
		pinctrl2 = "/soc/pinctrl@3860000";
		pinctrl3 = "/soc/pinctrl@106e0000";
		fimc-lite0 = "/soc/camera/fimc-lite@12390000";
		fimc-lite1 = "/soc/camera/fimc-lite@123a0000";
		mshc0 = "/soc/mmc@12550000";
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		ranges;

		clock-controller@3810000 {
			compatible = "samsung,exynos4210-audss-clock";
			reg = <0x3810000 0xc>;
			#clock-cells = <0x1>;
			clocks = <0x2 0x3 0x2 0x6 0x2 0x90 0x2 0x90>;
			clock-names = "pll_ref", "pll_in", "sclk_audio", "sclk_pcm_in";
			assigned-clocks = <0x3 0x0 0x3 0x1 0x3 0x2 0x3 0x3 0x3 0x4>;
			assigned-clock-parents = <0x2 0x6 0x3 0x0>;
			assigned-clock-rates = <0x0 0x0 0xbb80001 0x5dc0000 0x1770000>;
			phandle = <0x3>;
		};

		i2s@3830000 {
			compatible = "samsung,s5pv210-i2s";
			reg = <0x3830000 0x100>;
			clocks = <0x3 0x6 0x3 0x3 0x3 0x7>;
			clock-names = "iis", "i2s_opclk0", "i2s_opclk1";
			#clock-cells = <0x1>;
			clock-output-names = "i2s_cdclk0";
			dmas = <0x4 0xc 0x4 0xb 0x4 0xa>;
			dma-names = "tx", "rx", "tx-sec";
			samsung,idma-addr = <0x3000000>;
			#sound-dai-cells = <0x1>;
			status = "okay";
			pinctrl-0 = <0x5>;
			pinctrl-names = "default";
			assigned-clocks = <0x6 0x1>;
			assigned-clock-parents = <0x3 0x7>;
			phandle = <0x6>;
		};

		chipid@10000000 {
			compatible = "samsung,exynos4210-chipid";
			reg = <0x10000000 0x100>;
		};

		snoop-control-unit@10500000 {
			compatible = "arm,cortex-a9-scu";
			reg = <0x10500000 0x2000>;
		};

		memory-controller@12570000 {
			compatible = "samsung,exynos4210-srom";
			reg = <0x12570000 0x14>;
		};

		video-phy {
			compatible = "samsung,s5pv210-mipi-video-phy";
			#phy-cells = <0x1>;
			syscon = <0x7>;
			phandle = <0xa>;
		};

		mfc-power-domain@10023c40 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c40 0x20>;
			#power-domain-cells = <0x0>;
			label = "MFC";
			phandle = <0x25>;
		};

		g3d-power-domain@10023c60 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c60 0x20>;
			#power-domain-cells = <0x0>;
			label = "G3D";
		};

		lcd0-power-domain@10023c80 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c80 0x20>;
			#power-domain-cells = <0x0>;
			label = "LCD0";
			phandle = <0x8>;
		};

		tv-power-domain@10023c20 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c20 0x20>;
			#power-domain-cells = <0x0>;
			power-domains = <0x8>;
			label = "TV";
			phandle = <0x3d>;
		};

		cam-power-domain@10023c00 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023c00 0x20>;
			#power-domain-cells = <0x0>;
			label = "CAM";
			phandle = <0xb>;
		};

		gps-power-domain@10023ce0 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023ce0 0x20>;
			#power-domain-cells = <0x0>;
			label = "GPS";
		};

		gps-alive-power-domain@10023d00 {
			compatible = "samsung,exynos4210-pd";
			reg = <0x10023d00 0x20>;
			#power-domain-cells = <0x0>;
			label = "GPS alive";
		};

		interrupt-controller@10490000 {
			compatible = "arm,cortex-a9-gic";
			#interrupt-cells = <0x3>;
			interrupt-controller;
			reg = <0x10490000 0x10000 0x10480000 0x10000>;
			cpu-offset = <0x4000>;
			phandle = <0x1>;
		};

		interrupt-controller@10440000 {
			compatible = "samsung,exynos4210-combiner";
			#interrupt-cells = <0x2>;
			interrupt-controller;
			reg = <0x10440000 0x1000>;
			samsung,combiner-nr = <0x14>;
			interrupts = <0x0 0x0 0x4 0x0 0x1 0x4 0x0 0x2 0x4 0x0 0x3 0x4 0x0 0x4 0x4 0x0 0x5 0x4 0x0 0x6 0x4 0x0 0x7 0x4 0x0 0x8 0x4 0x0 0x9 0x4 0x0 0xa 0x4 0x0 0xb 0x4 0x0 0xc 0x4 0x0 0xd 0x4 0x0 0xe 0x4 0x0 0xf 0x4 0x0 0x6b 0x4 0x0 0x6c 0x4 0x0 0x30 0x4 0x0 0x2a 0x4>;
			phandle = <0x9>;
		};

		pmu {
			compatible = "arm,cortex-a9-pmu";
			interrupt-parent = <0x9>;
			interrupts = <0x2 0x2 0x3 0x2 0x12 0x2 0x13 0x2>;
		};

		syscon@10010000 {
			compatible = "samsung,exynos4-sysreg", "syscon";
			reg = <0x10010000 0x400>;
			phandle = <0xc>;
		};

		system-controller@10020000 {
			compatible = "samsung,exynos4412-pmu", "syscon";
			reg = <0x10020000 0x4000>;
			interrupt-controller;
			#interrupt-cells = <0x3>;
			interrupt-parent = <0x1>;
			clock-names = "clkout0", "clkout1", "clkout2", "clkout3", "clkout4", "clkout8", "clkout9";
			clocks = <0x2 0x17 0x2 0x18 0x2 0x19 0x2 0x1a 0x2 0x1b 0x2 0x1 0x2 0x2>;
			#clock-cells = <0x1>;
			phandle = <0x7>;

			syscon-poweroff {
				compatible = "syscon-poweroff";
				regmap = <0x7>;
				offset = <0x330c>;
				mask = <0x5200>;
			};

			syscon-reboot {
				compatible = "syscon-reboot";
				regmap = <0x7>;
				offset = <0x400>;
				mask = <0x1>;
			};
		};

		dsi@11c80000 {
			compatible = "samsung,exynos4210-mipi-dsi";
			reg = <0x11c80000 0x10000>;
			interrupts = <0x0 0x4f 0x4>;
			power-domains = <0x8>;
			phys = <0xa 0x1>;
			phy-names = "dsim";
			clocks = <0x2 0x11e 0x2 0x8f>;
			clock-names = "bus_clk", "sclk_mipi";
			status = "disabled";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
		};

		camera {
			compatible = "samsung,fimc", "simple-bus";
			status = "okay";
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			#clock-cells = <0x1>;
			clock-output-names = "cam_a_clkout", "cam_b_clkout";
			ranges;
			clocks = <0x2 0x84 0x2 0x85 0x2 0x15f 0x2 0x160>;
			clock-names = "sclk_cam0", "sclk_cam1", "pxl_async0", "pxl_async1";
			pinctrl-names = "default";
			pinctrl-0;

			fimc@11800000 {
				compatible = "samsung,exynos4212-fimc";
				reg = <0x11800000 0x1000>;
				interrupts = <0x0 0x54 0x4>;
				clocks = <0x2 0x100 0x2 0x80>;
				clock-names = "fimc", "sclk_fimc";
				power-domains = <0xb>;
				samsung,sysreg = <0xc>;
				iommus = <0xd>;
				status = "okay";
				samsung,pix-limits = <0x1080 0x2000 0x780 0x1080>;
				samsung,mainscaler-ext;
				samsung,isp-wb;
				samsung,cam-if;
				assigned-clocks = <0x2 0x180 0x2 0x80>;
				assigned-clock-parents = <0x2 0x11>;
				assigned-clock-rates = <0x0 0xa7d8c00>;
			};

			fimc@11810000 {
				compatible = "samsung,exynos4212-fimc";
				reg = <0x11810000 0x1000>;
				interrupts = <0x0 0x55 0x4>;
				clocks = <0x2 0x101 0x2 0x81>;
				clock-names = "fimc", "sclk_fimc";
				power-domains = <0xb>;
				samsung,sysreg = <0xc>;
				iommus = <0xe>;
				status = "okay";
				samsung,pix-limits = <0x1080 0x2000 0x780 0x1080>;
				samsung,mainscaler-ext;
				samsung,isp-wb;
				samsung,cam-if;
				assigned-clocks = <0x2 0x181 0x2 0x81>;
				assigned-clock-parents = <0x2 0x11>;
				assigned-clock-rates = <0x0 0xa7d8c00>;
			};

			fimc@11820000 {
				compatible = "samsung,exynos4212-fimc";
				reg = <0x11820000 0x1000>;
				interrupts = <0x0 0x56 0x4>;
				clocks = <0x2 0x102 0x2 0x82>;
				clock-names = "fimc", "sclk_fimc";
				power-domains = <0xb>;
				samsung,sysreg = <0xc>;
				iommus = <0xf>;
				status = "okay";
				samsung,pix-limits = <0x1080 0x2000 0x780 0x1080>;
				samsung,mainscaler-ext;
				samsung,isp-wb;
				samsung,lcd-wb;
				samsung,cam-if;
				assigned-clocks = <0x2 0x182 0x2 0x82>;
				assigned-clock-parents = <0x2 0x11>;
				assigned-clock-rates = <0x0 0xa7d8c00>;
			};

			fimc@11830000 {
				compatible = "samsung,exynos4212-fimc";
				reg = <0x11830000 0x1000>;
				interrupts = <0x0 0x57 0x4>;
				clocks = <0x2 0x103 0x2 0x83>;
				clock-names = "fimc", "sclk_fimc";
				power-domains = <0xb>;
				samsung,sysreg = <0xc>;
				iommus = <0x10>;
				status = "okay";
				samsung,pix-limits = <0x780 0x2000 0x556 0x780>;
				samsung,rotators = <0x0>;
				samsung,mainscaler-ext;
				samsung,isp-wb;
				samsung,lcd-wb;
				assigned-clocks = <0x2 0x183 0x2 0x83>;
				assigned-clock-parents = <0x2 0x11>;
				assigned-clock-rates = <0x0 0xa7d8c00>;
			};

			csis@11880000 {
				compatible = "samsung,exynos4210-csis";
				reg = <0x11880000 0x4000>;
				interrupts = <0x0 0x4e 0x4>;
				clocks = <0x2 0x104 0x2 0x86>;
				clock-names = "csis", "sclk_csis";
				bus-width = <0x4>;
				power-domains = <0xb>;
				phys = <0xa 0x0>;
				phy-names = "csis";
				status = "disabled";
				#address-cells = <0x1>;
				#size-cells = <0x0>;
			};

			csis@11890000 {
				compatible = "samsung,exynos4210-csis";
				reg = <0x11890000 0x4000>;
				interrupts = <0x0 0x50 0x4>;
				clocks = <0x2 0x105 0x2 0x87>;
				clock-names = "csis", "sclk_csis";
				bus-width = <0x2>;
				power-domains = <0xb>;
				phys = <0xa 0x2>;
				phy-names = "csis";
				status = "disabled";
				#address-cells = <0x1>;
				#size-cells = <0x0>;
			};

			fimc-lite@12390000 {
				compatible = "samsung,exynos4212-fimc-lite";
				reg = <0x12390000 0x1000>;
				interrupts = <0x0 0x69 0x4>;
				power-domains = <0x11>;
				clocks = <0x12 0x4>;
				clock-names = "flite";
				iommus = <0x13>;
				status = "disabled";
			};

			fimc-lite@123a0000 {
				compatible = "samsung,exynos4212-fimc-lite";
				reg = <0x123a0000 0x1000>;
				interrupts = <0x0 0x6a 0x4>;
				power-domains = <0x11>;
				clocks = <0x12 0x5>;
				clock-names = "flite";
				iommus = <0x14>;
				status = "disabled";
			};

			fimc-is@12000000 {
				compatible = "samsung,exynos4212-fimc-is";
				reg = <0x12000000 0x260000>;
				interrupts = <0x0 0x5a 0x4 0x0 0x5f 0x4>;
				power-domains = <0x11>;
				clocks = <0x12 0x4 0x12 0x5 0x12 0xe 0x12 0xd 0x12 0x1 0x12 0x2 0x12 0x3 0x12 0x6 0x12 0x7 0x12 0xf 0x12 0x14 0x12 0x1b 0x12 0x1c 0x12 0x1d 0x12 0x1e 0x2 0x11 0x2 0xd 0x2 0x18b 0x2 0x1c6 0x2 0x1c7 0x2 0x17e>;
				clock-names = "lite0", "lite1", "ppmuispx", "ppmuispmx", "isp", "drc", "fd", "mcuisp", "gicisp", "mcuctl_isp", "pwm_isp", "ispdiv0", "ispdiv1", "mcuispdiv0", "mcuispdiv1", "mpll", "aclk200", "aclk400mcuisp", "div_aclk200", "div_aclk400mcuisp", "uart";
				iommus = <0x15 0x16 0x17 0x18>;
				iommu-names = "isp", "drc", "fd", "mcuctl";
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				ranges;
				status = "disabled";

				pmu@10020000 {
					reg = <0x10020000 0x3000>;
				};

				i2c-isp@12140000 {
					compatible = "samsung,exynos4212-i2c-isp";
					reg = <0x12140000 0x100>;
					clocks = <0x12 0x12>;
					clock-names = "i2c_isp";
					#address-cells = <0x1>;
					#size-cells = <0x0>;
				};
			};
		};

		rtc@10070000 {
			compatible = "samsung,s3c6410-rtc";
			reg = <0x10070000 0x100>;
			interrupt-parent = <0x7>;
			interrupts = <0x0 0x2c 0x4 0x0 0x2d 0x4>;
			clocks = <0x2 0x15a 0x19 0x0>;
			clock-names = "rtc", "rtc_src";
			status = "okay";
		};

		keypad@100a0000 {
			compatible = "samsung,s5pv210-keypad";
			reg = <0x100a0000 0x100>;
			interrupts = <0x0 0x6d 0x4>;
			clocks = <0x2 0x15b>;
			clock-names = "keypad";
			status = "disabled";
		};

		sdhci@12510000 {
			compatible = "samsung,exynos4210-sdhci";
			reg = <0x12510000 0x100>;
			interrupts = <0x0 0x49 0x4>;
			clocks = <0x2 0x129 0x2 0x91>;
			clock-names = "hsmmc", "mmc_busclk.2";
			status = "disabled";
		};

		sdhci@12520000 {
			compatible = "samsung,exynos4210-sdhci";
			reg = <0x12520000 0x100>;
			interrupts = <0x0 0x4a 0x4>;
			clocks = <0x2 0x12a 0x2 0x92>;
			clock-names = "hsmmc", "mmc_busclk.2";
			status = "disabled";
		};

		sdhci@12530000 {
			compatible = "samsung,exynos4210-sdhci";
			reg = <0x12530000 0x100>;
			interrupts = <0x0 0x4b 0x4>;
			clocks = <0x2 0x12b 0x2 0x93>;
			clock-names = "hsmmc", "mmc_busclk.2";
			status = "okay";
			bus-width = <0x4>;
			pinctrl-0 = <0x1a 0x1b 0x1c 0x1d>;
			pinctrl-names = "default";
			vmmc-supply = <0x1e>;
			vqmmc-supply = <0x1f>;
			cd-gpios = <0x20 0x2 0x0>;
			cd-inverted;
		};

		sdhci@12540000 {
			compatible = "samsung,exynos4210-sdhci";
			reg = <0x12540000 0x100>;
			interrupts = <0x0 0x4c 0x4>;
			clocks = <0x2 0x12c 0x2 0x94>;
			clock-names = "hsmmc", "mmc_busclk.2";
			status = "disabled";
		};

		exynos-usbphy@125b0000 {
			compatible = "samsung,exynos4x12-usb2-phy";
			reg = <0x125b0000 0x100>;
			samsung,pmureg-phandle = <0x7>;
			clocks = <0x2 0x131 0x2 0x2>;
			clock-names = "phy", "ref";
			#phy-cells = <0x1>;
			status = "okay";
			samsung,sysreg-phandle = <0xc>;
			phandle = <0x21>;
		};

		hsotg@12480000 {
			compatible = "samsung,s3c6400-hsotg";
			reg = <0x12480000 0x20000>;
			interrupts = <0x0 0x47 0x4>;
			clocks = <0x2 0x131>;
			clock-names = "otg";
			phys = <0x21 0x0>;
			phy-names = "usb2-phy";
			status = "okay";
			dr_mode = "peripheral";
			vusb_d-supply = <0x22>;
			vusb_a-supply = <0x23>;
		};

		ehci@12580000 {
			compatible = "samsung,exynos4210-ehci";
			reg = <0x12580000 0x100>;
			interrupts = <0x0 0x46 0x4>;
			clocks = <0x2 0x130>;
			clock-names = "usbhost";
			status = "okay";
			#address-cells = <0x1>;
			#size-cells = <0x0>;

			port@0 {
				reg = <0x0>;
				phys = <0x21 0x1>;
				status = "disabled";
			};

			port@1 {
				reg = <0x1>;
				phys = <0x21 0x2>;
				status = "okay";
			};

			port@2 {
				reg = <0x2>;
				phys = <0x21 0x3>;
				status = "disabled";
			};
		};

		ohci@12590000 {
			compatible = "samsung,exynos4210-ohci";
			reg = <0x12590000 0x100>;
			interrupts = <0x0 0x46 0x4>;
			clocks = <0x2 0x130>;
			clock-names = "usbhost";
			status = "disabled";
			#address-cells = <0x1>;
			#size-cells = <0x0>;

			port@0 {
				reg = <0x0>;
				phys = <0x21 0x1>;
				status = "disabled";
			};
		};

		i2s@13960000 {
			compatible = "samsung,s3c6410-i2s";
			reg = <0x13960000 0x100>;
			clocks = <0x2 0x14a>;
			clock-names = "iis";
			#clock-cells = <0x1>;
			clock-output-names = "i2s_cdclk1";
			dmas = <0x24 0xc 0x24 0xb>;
			dma-names = "tx", "rx";
			#sound-dai-cells = <0x1>;
			status = "disabled";
		};

		i2s@13970000 {
			compatible = "samsung,s3c6410-i2s";
			reg = <0x13970000 0x100>;
			clocks = <0x2 0x14b>;
			clock-names = "iis";
			#clock-cells = <0x1>;
			clock-output-names = "i2s_cdclk2";
			dmas = <0x4 0xe 0x4 0xd>;
			dma-names = "tx", "rx";
			#sound-dai-cells = <0x1>;
			status = "disabled";
		};

		codec@13400000 {
			compatible = "samsung,mfc-v5";
			reg = <0x13400000 0x10000>;
			interrupts = <0x0 0x5e 0x4>;
			power-domains = <0x25>;
			clocks = <0x2 0x111 0x2 0xaa>;
			clock-names = "mfc", "sclk_mfc";
